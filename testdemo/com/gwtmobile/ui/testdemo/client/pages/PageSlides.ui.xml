<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder 
	xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:com.gwtmobile.ui.client.widgets"
	>
	<ui:style>
	</ui:style>
	<m:HTMLPanel>
		<m:HeaderPanel>
			<m:HorizontalPanel>
				<m:VerticalPanel>
					<m:Label text="GWT Mobile" labelTemplate="Emphasised"/>
					<m:Label labelTemplate="Legend" text="Demo for Labels with variants"/>
				</m:VerticalPanel>
			</m:HorizontalPanel>
			<m:Button caption="Back" iconImage="Back" iconPosition="Left" ui:field="backButton">Back</m:Button>
		</m:HeaderPanel>
		<m:SlidePanel >
			<m:Slide>
				<m:Label text="slide1"/>
			</m:Slide>
			<m:Slide>
				<m:Label text="slide2"/>
			</m:Slide>
			<m:Slide>
				<m:Label text="slide3"/>
			</m:Slide>
		</m:SlidePanel>
	</m:HTMLPanel>	
</ui:UiBinder> 
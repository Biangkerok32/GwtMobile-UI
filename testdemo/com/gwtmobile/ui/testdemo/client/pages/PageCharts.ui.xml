<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder 
	xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:com.gwtmobile.ui.client.widgets" 
	>
	<ui:style>
	</ui:style>
	<m:HTMLPanel>
		<m:HeaderPanel>
			<m:HorizontalPanel>
				<m:VerticalPanel>
					<m:Label text="GWT Mobile" labelTemplate="Emphasised"/>
					<m:Label labelTemplate="Legend" text="Demo for Labels with variants"/>
				</m:VerticalPanel>
			</m:HorizontalPanel>
			<m:Button caption="Back" iconImage="Back" iconPosition="Left" ui:field="backButton">Back</m:Button>
		</m:HeaderPanel>
		<m:ScrollPanel>
			<m:VerticalPanel>
				<m:PieChart chartHeight="120" chartWidth="300" chartTitle="Hello" chartPie3D="true"/>
			</m:VerticalPanel>
		</m:ScrollPanel>
	</m:HTMLPanel>	
</ui:UiBinder> 
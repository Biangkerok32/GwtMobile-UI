<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:com.gwtmobile.ui.client.widgets" 
	>
	<ui:style>
	</ui:style>
	<m:HTMLPanel>
		<m:HeaderPanel>
			<m:HorizontalPanel boxPack="start" forceFullWidth="true" withVPadding="true">
				<m:Image url="logo.png"/>
				<m:VerticalPanel>
					<m:Label horizontalAlignment="ALIGN_LEFT" text="GWT Mobile" labelTemplate="Header1">Hello GWT Mobile UI!</m:Label>
					<m:Label text="GWT framework for Mobile Applications" labelTemplate="Legend" wordWrap="false"/>
				</m:VerticalPanel>
			</m:HorizontalPanel>
				
			
		</m:HeaderPanel>

		<m:ScrollPanel>
			
			<m:ListPanel ui:field="list" displayArrow="Visible">
				<m:ListItem displayArrow="Hidden">
					<m:HorizontalPanel boxPack="justify">
						<m:HorizontalPanel withVPadding="true">
							<m:Image iconImages="Home" height="2em"/>
							<m:Label text="Buttons &amp; Flip Switch" withHPadding="true">Buttons</m:Label>
						</m:HorizontalPanel>
						<m:Image iconImages="ArrowRight" height="1.2em"/>
					</m:HorizontalPanel></m:ListItem>		
									<m:ListItem displayArrow="Hidden">
					<m:HorizontalPanel boxPack="justify">
						<m:HorizontalPanel withVPadding="true">
							<m:Image iconImages="Delete" height="2em"/>
							<m:Label text="Labels Variants" withHPadding="true">Panels</m:Label>
						</m:HorizontalPanel>
						<m:Image iconImages="ArrowRight" height="1.2em"/>
					</m:HorizontalPanel></m:ListItem>			
				<m:ListItem displayArrow="Hidden">
					<m:HorizontalPanel boxPack="justify">
						<m:HorizontalPanel withVPadding="true">
							<m:Image iconImages="Search" height="2em"/>
							<m:Label text="Tabs on Top" withHPadding="true"/>
						</m:HorizontalPanel>
						<m:Image height="1.2em" iconImages="ArrowRight"/>
					</m:HorizontalPanel></m:ListItem>
				<m:ListItem displayArrow="Hidden">
					<m:HorizontalPanel boxPack="justify">
						<m:HorizontalPanel withVPadding="true">
							<m:Image iconImages="Check" height="2em"/>
							<m:Label text="Tabs on Bottom" withHPadding="true"/>
						</m:HorizontalPanel>
						<m:Image height="1.2em" iconImages="ArrowRight"/>
					</m:HorizontalPanel>
				</m:ListItem>
				<m:ListItem displayArrow="Hidden">
					<m:HorizontalPanel boxPack="justify">
						<m:HorizontalPanel withVPadding="true">
							<m:Image iconImages="Refresh" height="2em"/>
							<m:Label text="Stack List" withHPadding="true"/>
						</m:HorizontalPanel>
						<m:Image height="1.2em" iconImages="ArrowRight"/>
					</m:HorizontalPanel>
				</m:ListItem>
				<m:ListItem displayArrow="Hidden">
					<m:HorizontalPanel boxPack="justify">
						<m:HorizontalPanel withVPadding="true">
							<m:Image height="2em" iconImages="Star"/>
							<m:Label text="Forms" withHPadding="true"/>
						</m:HorizontalPanel>
						<m:Image height="1.2em" iconImages="ArrowRight"/>
					</m:HorizontalPanel>
				</m:ListItem>
				<m:ListItem displayArrow="Hidden">
					<m:HorizontalPanel boxPack="justify">
						<m:HorizontalPanel withVPadding="true">
							<m:Image height="2em" iconImages="Alert"/>
							<m:Label text="Slides" withHPadding="true"/>
						</m:HorizontalPanel>
						<m:Image height="1.2em" iconImages="ArrowRight"/>
					</m:HorizontalPanel>
				</m:ListItem>
				<m:ListItem displayArrow="Hidden">
					<m:HorizontalPanel boxPack="justify">
						<m:HorizontalPanel withVPadding="true">
							<m:Image height="2em" iconImages="Grid"/>
							<m:Label text="Grid Panel" withHPadding="true"/>
						</m:HorizontalPanel>
						<m:Image height="1.2em" iconImages="ArrowRight"/>
					</m:HorizontalPanel>
				</m:ListItem>
				<m:ListItem displayArrow="Hidden">
					<m:HorizontalPanel boxPack="justify">
						<m:HorizontalPanel withVPadding="true">
							<m:Image height="2em" iconImages="Grid"/>
							<m:Label text="Charts" withHPadding="true"/>
						</m:HorizontalPanel>
						<m:Image height="1.2em" iconImages="ArrowRight"/>
					</m:HorizontalPanel>
				</m:ListItem>		
			</m:ListPanel>


		</m:ScrollPanel>
		

	</m:HTMLPanel>	
</ui:UiBinder> 
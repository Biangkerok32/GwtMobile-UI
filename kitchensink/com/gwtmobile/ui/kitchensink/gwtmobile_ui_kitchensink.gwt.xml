<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.1.1//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.1.1/distro-source/core/src/gwt-module.dtd">
<module rename-to='KitchenSink'>
	<inherits name="com.google.gwt.user.User" />
		
	<source path="client" />
	<entry-point
		class="com.gwtmobile.ui.kitchensink.client.KitchenSink">
	</entry-point>
	
	<!-- Inherit GWT Mobile UI module -->
	<inherits name='com.gwtmobile.ui.gwtmobile_ui'/>
	
	<!-- Only compile WebKit permutation (for now) -->
	<set-property name="user.agent" value="safari"/>
	
</module>
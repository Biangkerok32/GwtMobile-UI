<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:com.gwtmobile.ui.client.widgets">
	<ui:style>
	</ui:style>
	<m:PagePanel>
		<m:HeaderPanel ui:field="header" leftButton="Back" rightButton="Next">
			<g:Label>Slide Panel</g:Label>			
		</m:HeaderPanel>
		<m:SlidePanel ui:field="slider" currentSlide="1">
			<m:Slide>
				<g:HTML>Slide Me!</g:HTML>
				<g:HTML>Static Slide 0</g:HTML>
			</m:Slide>	
			<m:Slide>
				<g:HTML>Slide Me!</g:HTML>
				<g:HTML>Static Slide 1</g:HTML>
				<g:Label text="Test"/>
			</m:Slide>	
		</m:SlidePanel>		
	</m:PagePanel>	
</ui:UiBinder> 
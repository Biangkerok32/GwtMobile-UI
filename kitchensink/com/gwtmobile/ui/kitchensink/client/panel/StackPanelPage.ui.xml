<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:com.gwtmobile.ui.client.widgets">
	<ui:style>
	</ui:style>
	<g:HTMLPanel>
		<m:HeaderPanel ui:field="header" leftButton="Back">
			<g:Label>Stack Panel</g:Label>			
		</m:HeaderPanel>
		<m:ScrollPanel>
		<m:StackPanel ui:field="stack">
			<m:Stack>
				<m:StackHeader>
					<g:Label>Stack 1</g:Label>
				</m:StackHeader>
				<m:StackContent>
					<g:Label>Stack 1 Content</g:Label>
				</m:StackContent>
			</m:Stack>
			<m:Stack>
				<m:StackHeader>
					<g:Label>Stack 2</g:Label>
				</m:StackHeader>
				<m:StackContent>
					<g:Label>Stack 2 Content</g:Label>
					<g:Label>Stack 2 Content</g:Label>
				</m:StackContent>
			</m:Stack>
			<m:Stack>
				<m:StackHeader>
					<g:Label>Stack 3</g:Label>
				</m:StackHeader>
				<m:StackContent>
					<g:Label>Stack 3 Content</g:Label>
					<g:Label>Stack 3 Content</g:Label>
					<g:Label>Stack 3 Content</g:Label>
				</m:StackContent>
			</m:Stack>
			<m:Stack>
				<m:StackHeader>
					<g:Label>Stack 4</g:Label>
				</m:StackHeader>
				<m:StackContent>
					<g:Label>Stack 4 Content</g:Label>
					<g:Label>Stack 4 Content</g:Label>
					<g:Label>Stack 4 Content</g:Label>
					<g:Label>Stack 4 Content</g:Label>
				</m:StackContent>
			</m:Stack>
		</m:StackPanel>
		</m:ScrollPanel>
	</g:HTMLPanel>	
</ui:UiBinder> 